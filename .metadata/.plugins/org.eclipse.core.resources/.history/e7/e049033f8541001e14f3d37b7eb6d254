
public class P7 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		// 증감 연산자
		int  a = 10, b = 10, x, y;
        a++;
        ++b;
        System.out.println("a:"+a+", b:"+b);

        x = --a;
        y = b--;
        System.out.println("a:"+a+", b:"+b);
        System.out.println("x:"+x+", y:"+y);
	}

}
